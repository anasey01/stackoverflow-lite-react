{
  "name": "stackoverflow-lite-react",
  "version": "1.0.0",
  "description": "frontend implementation of stackoverflow lite using reactjs",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run heroku:prebuild && webpack && npm run prebuild && node server/dist/app.js",
    "test": "jest --config ./jest.config.json --coverage --colors",
    "test:watch": "jest --config ./jest.config.json --watch",
    "dev": "webpack-dev-server --open",
    "prebuild": "rm -rf server/dist && babel server --out-dir server/dist",
    "heroku:prebuild": "npm install --only=dev",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anasey01/stackoverflow-lite-react.git"
  },
  "keywords": [
    "andela",
    "stackoverflow-lite",
    "bootcamp"
  ],
  "author": "Oluseyi Anani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anasey01/stackoverflow-lite-react/issues"
  },
  "homepage": "https://github.com/anasey01/stackoverflow-lite-react#readme",
